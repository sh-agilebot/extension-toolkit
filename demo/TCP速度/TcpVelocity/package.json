{
  "name": "tcp-velocity-extension",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "dev": "run-p \"dev:*\"",
    "dev:backend": "python main.py",
    "dev:frontend": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "run-p \"preview:*\"",
    "preview:serve": "python dist/main.py",
    "preview:open": "wait-on tcp:8000 && open-cli http://localhost:8000",
    "build-only": "vite build",
    "type-check": "vue-tsc --build"
  },
  "dependencies": {
    "@agilebot/extension-runtime": "^1.0.0-beta.13",
    "@vueuse/core": "^14.0.0",
    "axios": "^1.13.2",
    "tailwindcss": "^4.1.17",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.12"
  },
  "devDependencies": {
    "@agilebot/extension-schemas": "^1.0.0-beta.4",
    "@antfu/eslint-config": "^6.2.0",
    "@tailwindcss/vite": "^4.1.17",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.13.9",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.39.1",
    "npm-run-all2": "^8.0.4",
    "open-cli": "^8.0.0",
    "typescript": "~5.8.0",
    "vite": "^7.2.1",
    "vite-plugin-static-copy": "^3.1.4",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^3.1.3",
    "wait-on": "^9.0.3"
  }
}
